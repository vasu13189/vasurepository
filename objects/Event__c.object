    <validationRules>
        <fullName>Must_Select_Frequency</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    Recurring__c ,
    ISBLANK( TEXT( Frequency__c ) )
)</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>You must need to Select Frequency field for Recurring events</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>You_can_Select_Frequency</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
     NOT Recurring__c ,
     NOT ISBLANK( TEXT ( Frequency__c ) ) 
)</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>You can Select Frequency for Non-Recurring Events</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Please_select_a_location_for_InPerson</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   TEXT (  Event_Type__c  ) != &apos;Virtual&apos;,
   ISBLANK (  Location__c )
)</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>Please select a location for In-Person Event</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_must_be_Greater_than_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
   NOT ISBLANK (  End_Date_Time__c  ),
   End_Date_Time__c &lt;=  (Start_DateTime__c + 1)
)</errorConditionFormula>
        <errorDisplayField>End_Date_Time__c</errorDisplayField>
        <errorMessage>End Date must be  greater than Start Date/Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Need_to_Select_Location_If_not_Virt</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   TEXT (  Event_Type__c  ) = &apos;Virtual&apos;,
   NOT ISBLANK (  Location__c )
)</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>You can not Select the Location if the Event is Virtual</errorMessage>
    </validationRules>
</CustomObject>